{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "mgws(hq.local)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/apps/webrtc/webrtc",
            "args": [
                "${workspaceFolder}/www",
                "${workspaceFolder}/certs/hq.local.crt",
                "${workspaceFolder}/certs/hq.local.key",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/certs",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "mgws(hq.e-man.tv)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/apps/webrtc/webrtc",
            "args": [
                "${workspaceFolder}/www",
                "${env:NGINX_CERTS_DIR}/fullchain.pem",
                "${env:NGINX_CERTS_DIR}/privkey.pem",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/certs",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}